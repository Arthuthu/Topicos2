
@{
    ViewBag.Title = "Alunos";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Alunos</h2>

@{
    var alunos = ViewBag.Alunos;
}

<div class="container">
    <div class="row">
        <ul class="list-group">
            @foreach (var aluno in alunos)
            {
               <li class="list-group-item">@aluno.FullName</li>
            }
        </ul>
    </div>
</div>


